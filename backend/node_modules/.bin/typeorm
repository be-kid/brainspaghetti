#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jooyong/brainspaghetti/backend/node_modules/.pnpm/typeorm@0.3.26_mysql2@3.14.5_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@22.18.1_typescript@5.9.2_/node_modules/typeorm/node_modules:/Users/jooyong/brainspaghetti/backend/node_modules/.pnpm/typeorm@0.3.26_mysql2@3.14.5_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@22.18.1_typescript@5.9.2_/node_modules:/Users/jooyong/brainspaghetti/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jooyong/brainspaghetti/backend/node_modules/.pnpm/typeorm@0.3.26_mysql2@3.14.5_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@22.18.1_typescript@5.9.2_/node_modules/typeorm/node_modules:/Users/jooyong/brainspaghetti/backend/node_modules/.pnpm/typeorm@0.3.26_mysql2@3.14.5_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@22.18.1_typescript@5.9.2_/node_modules:/Users/jooyong/brainspaghetti/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli.js" "$@"
else
  exec node  "$basedir/../typeorm/cli.js" "$@"
fi
